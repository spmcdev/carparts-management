{
  "projects": [
    {
      "displayName": "working-tests",
      "testEnvironment": "node",
      "testMatch": ["<rootDir>/tests/*.test.js"],
      "transform": {
        "^.+\\.js$": "babel-jest"
      },
      "globals": {
        "__DEV__": true
      },
      "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"]
    }
  ]
}
